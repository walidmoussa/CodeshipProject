APP_NAME={{ domain }}
APP_ENV=production
APP_KEY={{ app_key.stdout }}
APP_DEBUG=false
APP_LOG_LEVEL=debug
APP_URL=http://{{ domain }}

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE={{ mysql_db }}
DB_USERNAME={{ mysql_user }}
DB_PASSWORD={{ mysql_password }}

BROADCAST_DRIVER=redis
CACHE_DRIVER=file
SESSION_DRIVER=file
SESSION_LIFETIME=120
QUEUE_DRIVER=sync

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_DRIVER=smtp
MAIL_HOST=localhost
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
